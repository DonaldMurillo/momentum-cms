{
	"compileOnSave": false,
	"compilerOptions": {
		"rootDir": ".",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2015",
		"module": "esnext",
		"lib": ["es2020", "dom"],
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"baseUrl": ".",
		"paths": {
			"@momentumcms/core": ["libs/core/src/index.ts"],
			"@momentumcms/server-core": ["libs/server-core/src/index.ts"],
			"@momentumcms/server-express": ["libs/server-express/src/index.ts"],
			"@momentumcms/server-analog": ["libs/server-analog/src/index.ts"],
			"@momentumcms/admin": ["libs/admin/src/index.ts"],
			"@momentumcms/db-drizzle": ["libs/db-drizzle/src/index.ts"],
			"@momentumcms/migrations": ["libs/migrations/src/index.ts"],
			"@momentumcms/auth": ["libs/auth/src/index.ts"],
			"@momentumcms/auth/core": ["libs/auth/src/core.ts"],
			"@momentumcms/auth/collections": ["libs/auth/src/lib/auth-collections.ts"],
			"@momentumcms/ui": ["libs/ui/src/index.ts"],
			"@momentumcms/storage": ["libs/storage/src/index.ts"],
			"@momentumcms/e2e-fixtures": ["libs/e2e-fixtures/src/index.ts"],
			"@momentumcms/logger": ["libs/logger/src/index.ts"],
			"@momentumcms/plugins/core": ["libs/plugins/core/src/index.ts"],
			"@momentumcms/plugins/analytics": ["libs/plugins/analytics/src/index.ts"],
			"@momentumcms/plugins/otel": ["libs/plugins/otel/src/index.ts"],
			"@momentumcms/plugins/analytics/dashboard": [
				"libs/plugins/analytics/src/lib/dashboard/analytics-dashboard.page.ts"
			],
			"@momentumcms/plugins/analytics/admin-routes": [
				"libs/plugins/analytics/src/lib/analytics-admin-routes.ts"
			],
			"@momentumcms/plugins/analytics/client": ["libs/plugins/analytics/src/lib/client/tracker.ts"],
			"@momentumcms/plugins/analytics/block-fields": [
				"libs/plugins/analytics/src/lib/collectors/block-field-injector.ts"
			],
			"@momentumcms/example-config": ["libs/example-config/src/index.ts"],
			"@momentumcms/example-config/collections": ["libs/example-config/src/collections/index.ts"],
			"@momentumcms/example-config/globals": ["libs/example-config/src/globals/index.ts"],
			"@momentumcms/example-config/pages": ["libs/example-config/src/pages/index.ts"]
		}
	},
	"exclude": ["node_modules", "tmp"]
}
