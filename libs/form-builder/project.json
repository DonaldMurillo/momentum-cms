{
	"name": "form-builder",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/form-builder/src",
	"prefix": "mcms",
	"projectType": "library",
	"release": {
		"version": {
			"manifestRootsToUpdate": ["dist/{projectRoot}"],
			"currentVersionResolver": "git-tag",
			"fallbackCurrentVersionResolver": "disk"
		}
	},
	"tags": ["scope:lib", "env:browser"],
	"targets": {
		"build": {
			"executor": "@nx/angular:package",
			"outputs": ["{workspaceRoot}/dist/{projectRoot}"],
			"options": {
				"project": "libs/form-builder/ng-package.json",
				"tsConfig": "libs/form-builder/tsconfig.lib.json"
			},
			"configurations": {
				"production": {
					"tsConfig": "libs/form-builder/tsconfig.lib.prod.json"
				},
				"development": {}
			},
			"defaultConfiguration": "production"
		},
		"nx-release-publish": {
			"options": {
				"packageRoot": "dist/{projectRoot}",
				"access": "public"
			}
		},
		"test": {
			"executor": "@nx/angular:unit-test",
			"options": {}
		},
		"lint": {
			"executor": "@nx/eslint:lint"
		}
	}
}
