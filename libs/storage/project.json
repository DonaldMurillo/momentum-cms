{
	"name": "storage",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/storage/src",
	"projectType": "library",
	"tags": ["scope:lib", "env:server"],
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/storage",
				"main": "libs/storage/src/index.ts",
				"tsConfig": "libs/storage/tsconfig.lib.json"
			}
		},
		"test": {
			"executor": "@nx/vitest:test",
			"outputs": ["{workspaceRoot}/coverage/libs/storage"],
			"options": {
				"config": "libs/storage/vitest.config.mts"
			}
		},
		"nx-release-publish": {
			"options": {
				"packageRoot": "dist/{projectRoot}",
				"access": "public"
			}
		}
	}
}
